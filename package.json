{
  "name": "@githubnext/vscode-react-webviews",
  "version": "1.0.0",
  "description": "A sample/starter template for developing VS Code extensions with webviews",
  "main": "out/extension.cjs",
  "type": "module",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && node esbuild.config.js",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p",
    "format": "prettier --write **/*.ts",
    "dev": "vite build --watch"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/githubnext/vscode-react-webviews.git"
  },
  "keywords": [
    "vscode"
  ],
  "author": "Next Devex <next-devex@github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/githubnext/vscode-react-webviews.git/issues"
  },
  "homepage": "https://github.com/githubnext/vscode-react-webviews.git#readme",
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/tailwindcss": "^3.1.0",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.20.0",
    "eslint": "^9.15.0",
    "postcss": "^8.4.49",
    "postcss-nested": "^7.0.2",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@hediet/node-reload": "^0.8.0",
    "nanoid": "^5.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.28.0",
    "vite": "^5.4.11",
    "zustand": "^5.0.1"
  },
  "contributes": {
    "commands": [
      {
        "command": "NextWebview1.start",
        "title": "Open Webview 1",
        "category": "GitHub Next"
      },
      {
        "command": "NextWebview2.start",
        "title": "Open Webview 2",
        "category": "GitHub Next"
      }
    ]
  }
}